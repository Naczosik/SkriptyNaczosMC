    # add "<#79ceff>/discord link <white>- połącz konto na serwerze z kontem discord!" to {pomoc::*}
    
on join:
    wait 100 seconds
    while event-player is online:
        if {pref::helpinfo::%player%} is not set:
            send ""
            send "&8&l[<#FCC729><bold>!&8&l] <#FCC729><bold>INFO &8→ <white>%random element of {pomoc::*}%" to event-player
            send ""
        wait 300 seconds

